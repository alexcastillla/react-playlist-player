{
  "name": "react-playlist-player",
  "version": "1.0.0",
  "description": "react-playlist-player React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "jest"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "mobx": "^5.7.0",
    "mobx-react": "^5.4.2",
    "react-app-polyfill": "^0.1.3"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "nwb": "0.23.x",
    "nwb-sass": "^0.9.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.6.3"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>setupEnzyme.js"
  },
  "author": "Daniel Sarmiento Cordero",
  "homepage": "https://react-playlist-player.firebaseapp.com/",
  "license": "MIT",
  "repository": "https://github.com/devsarmico/react-playlist-player",
  "keywords": [
    "react-component",
    "music",
    "player",
    "music player",
    "playlist"
  ]
}
